<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--TCP链接-->
    <add key="TCP_StandByServerIP" value="192.168.100.180"/>
    <add key="TCP_ServerPort" value="8001"/>
    <!--主备机权限，main,standby-->
    <add key="Priority" value="main"/>
    <!--主备机广播端口-->
    <!--发送相似航班号的端口-->
    <add key="SendAlarmIp" value="192.168.100.255" />
    <add key="SendAlarmPort" value="21003" />
    <!--计算周期间隔-->
    <add key="intervalTime" value="4" />
    <add key="intervalDis" value="4" />
    <!--科室信息-->
    <add key="Office" value="HD006"/>
    <!--角色，一共分为三种Controller, //管制        Super, //主任        Guest, //访客，其中仅Super可发出控制,若存在多个控制，可以#号隔开-->
    <add key="Role" value="Super"/>
    <!--席位-->
    <add key="Seate" value="区管六室-SUP"/>
    <!--站点信息-->
    <add key="zhandianId" value="HD006"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
</configuration>